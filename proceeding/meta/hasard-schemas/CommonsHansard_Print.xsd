<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.parliament.uk/commons/hansard/print" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.parliament.uk/commons/hansard/print" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.34">
  <xs:annotation>
    <xs:documentation>
      Version:
      0.34
      Description:
      This document defines the structure of the XML output from
      the reporting application of Commons Hansard into the page makeup unit.
      The output from the page makeup unit will also conform to this schema
      hence the presence of quite a lot in this schema that will not be
      present in the input to the page makeup unit.
      Some known limitations:
      - Much of the structure is loosely defined.  This is because
      the initial draft was intentionally loose to force the print related
      aspect of the schema to remain the required structure to the document.
      This will gradually be tightened with individual definitions of the
      data reuse elements as they are required and can be incorporated
      transparently to the Hansard users.
      Changes from last version:
      - 	Added pbc attribute to Cover element
      0.35 (25/05/2010)
      - Allow hs_TimeCode element within hs_Para, Body, B tags, etc.
    </xs:documentation>
  </xs:annotation>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd" />
  <xs:import namespace="http://www.w3.org/1998/Math/MathML" schemaLocation="http://www.w3.org/Math/XMLSchema/mathml2/mathml2.xsd" />
  <!-- The only globally defined elements hence the only valid root elements. -->
  <xs:element name="HansardDoc" type="HOCDocumentStructure" />
  <xs:element name="Fragment" type="FragmentStructure" />
  <!-- Top level structures and types -->
  <xs:complexType name="HOCDocumentStructure">
    <xs:annotation>
      <xs:documentation>
			The top level element of Commons Hansard documents.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="House" type="HOCHouseStructure" />
    </xs:sequence>
    <xs:attribute name="type" type="HOCSystemType" />
  </xs:complexType>
  <xs:complexType name="HOCHouseStructure">
    <xs:annotation>
      <xs:documentation>
			The structure of a top level house element for HOC Hansard.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="System" type="HOCSystemStructure" />
    </xs:sequence>
    <xs:attribute name="name" type="HOCHouseType" use="required" />
  </xs:complexType>
  <xs:simpleType name="HOCHouseType">
    <xs:annotation>
      <xs:documentation>The posible house types for a HOC document.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Commons" />
      <xs:enumeration value="CommonsCommittees" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="HOCSystemStructure">
    <xs:annotation>
      <xs:documentation>
			The wrapper element for the system that a document belongs to.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="unbounded">
      <xs:element ref="Fragment" />
    </xs:sequence>
    <xs:attribute name="type" type="HOCSystemType" use="required" />
    <xs:attribute name="ColStart" type="xs:string" />
  </xs:complexType>
  <xs:simpleType name="HOCSystemType">
    <xs:annotation>
      <xs:documentation>The possible system types for HOC document.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Debate" />
      <xs:enumeration value="WestHall" />
      <xs:enumeration value="Writtens" />
      <xs:enumeration value="Committee" />
      <xs:enumeration value="WMS" />
      <xs:enumeration value="Correction" />
      <xs:enumeration value="Petitions" />
      <xs:enumeration value="Cover" />
      <xs:enumeration value="Index" />
      <xs:enumeration value="Government" />
      <xs:enumeration value="Back_Cover" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="FragmentStructure">
    <xs:annotation>
      <xs:documentation>The structure of a fragment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Header" type="HeaderStructure" minOccurs="0" />
      <xs:element name="Body" type="BodyStructure" minOccurs="0" />
      <xs:element name="Index" minOccurs="0">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="IndexStructure" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Contents" minOccurs="0">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="ContentsStructure" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Cover-wrapper" minOccurs="0">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="CoverStructure" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute ref="xml:space" />
    <xs:attribute name="__uri__" />
    <xs:attribute name="__lock__" />
    <xs:attribute name="__shareable__" />
    <xs:attribute name="__version__" />
  </xs:complexType>
